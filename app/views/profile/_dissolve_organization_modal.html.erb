<div id="dissolve_org_modal" class="modal fade tasks-actions-modal modal-with-loader">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
        <h4 class="modal-title">Do you really want to dissolve organization?</h4>
      </div>
      <div class="modal-footer">
        <div class="text-right">
          <button class="btn btn-default" data-bind="click: closeDissolveOrgModal, disable: createDissolveOrgRequestButtonDisabled">No</button>
          <button class="btn btn-primary" data-bind="click: createDissolveOrgRequest, disable: createDissolveOrgRequestButtonDisabled">Yes</button>
        </div>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
