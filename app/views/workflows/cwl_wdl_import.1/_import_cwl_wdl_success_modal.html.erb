<div id="import_cwl_wdl_success_modal" class="modal fade modal-with-loader">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header alert alert-success">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
        <h4 class="modal-title">App successfully created!</h4>
      </div>
      <div class="loader-container">
        <div class="loader">
          <i class="fa fa-spinner fa-spin"></i>
        </div>
      </div>
      <div class="modal-body">
        <p>
          <b>App URL:</b>
          <a target="_blank" data-bind="text: newWorkflowUid, attr: {href: newWorkflowURL}"></a>
        </p>
        <!-- ko if: newAssetUid -->
        <p>
          <b>Asset URL:</b>
          <a target="_blank" data-bind="text: newAssetUid, attr: {href: newAssetURL}"></a>
        </p>
        <!-- /ko -->
      </div>
      <div class="modal-footer">
        <a data-bind="attr: {href: newWorkflowURL}" target="_blank">
          <input type="button" class="btn btn-primary" tabindex="-1" value="Ok">
        </a>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
