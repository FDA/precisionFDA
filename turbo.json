{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "client:build": {
      "dependsOn": ["server:build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start:dev:api": {
      "cache": false,
      "persistent": true
    },
    "start:dev:worker": {
      "cache": false,
      "persistent": true
    },
    "start:dev:admin-platform-client": {
      "cache": false,
      "persistent": true
    },
    "build:api": {},
    "build:worker": {},
    "build:admin-platform-client": {},
    "start:api": {},
    "start:worker": {},
    "start:admin-platform-client": {},
    "lint": {},
    "test": {}
  }
}
