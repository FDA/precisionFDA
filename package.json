{
  "name": "precision-fda-ut",
  "version": "1.0.0",
  "description": "This package.json is only here to unit test JS/React/CoffeeScript part of the project.",
  "main": "index.js",
  "devDependencies": {
    "coffeescript": "^2.5.1",
    "jest": "^26.6.3",
    "knockout": "^3.5.1",
    "lodash": "^4.17.21",
    "y18n": "^4.0.1"
  },
  "scripts": {
    "test": "TZ=America/New_York jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "coffee"
    ],
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).(js|coffee)?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/client",
      "<rootDir>/vendor"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      ".*": "./test/javascript/preprocessor.js"
    }
  },
  "repository": {},
  "author": "",
  "license": "ISC"
}
